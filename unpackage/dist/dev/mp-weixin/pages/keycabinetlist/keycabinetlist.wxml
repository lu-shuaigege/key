<view class="keycabinetlist"><image class="dot-bg" src="../../static/dot-bg.png"></image><view class="keycabinetlist-top"><image src="{{avatarUrl}}" data-event-opts="{{[['tap',[['toLogout']]]]}}" bindtap="__e"></image><view class="keycabinetlist-top-r"><text class="keycabinetlist-top-r-name">{{nickName}}</text></view></view><view hidden="{{!(manageNew.status==1)}}" class="administratornews"><image src="../../static/keycabinetlist-horn.png"></image><view class="administratornews-con"><text class="name">{{manageNew.user}}</text><text class="news">{{manageNew.message}}</text></view><button data-event-opts="{{[['tap',[['lookAuthList']]]]}}" bindtap="__e">点击查看</button></view><block wx:for="{{keycabinetList}}" wx:for-item="item" wx:for-index="index" wx:key="cabinetno"><view><block wx:if="{{index<2}}"><view data-event-opts="{{[['tap',[['lookDetail',['$0'],[[['keycabinetList','cabinetno',item.cabinetno,'cabinetno']]]]]]]}}" class="keycabinet-normal" bindtap="__e"><view class="keycabinet-normal-top"><label></label><text class="keycabinet-normal-top-name">{{item.cabinetname+" "+item.cabinetno}}</text><view class="keycabinet-normal-top-r"><block wx:if="{{item.online.status==1}}"><image class="dot-green" src="../../static/dot-green.png"></image></block><block wx:if="{{item.online.status==0}}"><image class="dot-green" src="../../static/dot-grey.png"></image></block><block wx:if="{{item.online.status==1}}"><text>网络正常</text></block><block wx:if="{{item.online.status==0}}"><text class="exception-text">网络异常</text></block></view></view><view class="keycabinet-normal-top2"><text class="keycabinet-normal-top-num">{{"NO."+item.cabinetno}}</text><view class="keycabinet-normal-top-r"><block wx:if="{{item.alarm.status==0}}"><image src="../../static/light-green.png"></image></block><block wx:if="{{item.alarm.status==1}}"><image src="../../static/light-red.png"></image></block><block wx:if="{{item.alarm.status==0}}"><text>{{item.alarm.title}}</text></block><block wx:if="{{item.alarm.status==1}}"><text class="exception-text">{{item.alarm.title}}</text></block></view></view><view class="keycabinet-normal-con"><image src="../../static/default-cabinetlist.png" mode></image><view class="keycabinet-normal-con-r"><view class="progress"><text class="progress-name">钥匙总数</text><view class="progress-box"><progress percent="100" activeColor="#2fa4ff" active stroke-width="3"></progress></view><text class="progress-num">{{item.keynum}}</text></view><view class="progress"><text class="progress-name">使 用 中</text><view class="progress-box"><progress percent="{{item.usednum/item.keynum*100}}" activeColor="#999999" active stroke-width="3"></progress></view><text class="progress-num">{{item.usednum}}</text></view><view class="progress"><text class="progress-name">错  位</text><view class="progress-box"><progress percent="{{item.errornum/item.keynum*100}}" activeColor="#ff3232" active stroke-width="3"></progress></view><text class="progress-num">{{item.errornum}}</text></view></view></view></view></block></view></block><view class="keycabinetlist-line"><image src="../../static/keycabinetlist-line.png"></image></view><view class="keycabinetlist-con"><scroll-view class="keycabinetlist-con-p" scroll-x="true"><block wx:for="{{keycabinetList}}" wx:for-item="item" wx:for-index="index" wx:key="cabinetno"><block wx:if="{{index>1}}"><view data-event-opts="{{[['tap',[['lookDetail',['$0'],[[['keycabinetList','cabinetno',item.cabinetno,'cabinetno']]]]]]]}}" class="keycabinetlist-con-s" bindtap="__e"><image src="../../static/default-cabinetlist.png"></image><text>{{item.cabinetname}}</text><text>{{item.cabinetno}}</text></view></block></block></scroll-view></view></view>