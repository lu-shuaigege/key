page {
	background: #f8f8f8;
}
.login{
}
.login-top{
	width: 750rpx;
	height: 343rpx;
}
.login-top image{
	width: 750rpx;
	height: 343rpx;
}
.login-con{
	width: 690rpx;
	height: 600rpx;
	-webkit-border-radius: 8rpx;
	        border-radius: 8rpx;
	margin-left: 30rpx;
	margin-top: -146rpx;
	position: relative;
	background: url(data:image/png;base64,) no-repeat;
	background-size: 100% 100%;
}
.login-con-top{
	width: 100%;
	text-align: center;
}
.login-con-top image{
	width: 150rpx;
	height: 150rpx;
	border: solid 2rpx #ffffff;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	/* margin-left: 270upx; */
	margin-top: -75rpx;
}
.login-con-top .name{
	display: block;
	font-size: 30rpx;
	color: #333333;
	margin-top: 20rpx;
}
.login-con-top .info{
	display: block;
	font-size: 24rpx;
	color: #666666;
	margin-top: 20rpx;
}
.login-submit{
	margin-top: 150rpx;
}
.login-submit button{
	width: 590rpx;
	height: 80rpx;
	background-color: #0f84f1;
	-webkit-border-radius: 40rpx;
	        border-radius: 40rpx;
	font-size: 30rpx;
	color: #ffffff;
}
.keycabinet-bottom{
	width: 750rpx;
	height: 80rpx;
	line-height: 80rpx;
	background-color: #e1e1e1;
	text-align: center;
	/* margin-top: 50upx; */
	position: absolute;
	bottom: 0;
}
.keycabinet-bottom image{
	width: 41rpx;
	height: 40rpx;
	vertical-align: middle;
}
.keycabinet-bottom text{
	width: 1rpx;
	height: 30rpx;
	background-color: #666666;
	display: inline-block;
	vertical-align: middle;
	margin-left: 20rpx;
	margin-right: 20rpx;
}
.keycabinet-bottom label{
	font-size: 30rpx;
	color: #666666;
}

