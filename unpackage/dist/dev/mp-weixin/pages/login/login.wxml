<view class="login"><view class="login-top"><image src="../../static/login-bg.png"></image></view><view class="login-con"><view class="login-con-top"><block wx:if="{{avatarUrl}}"><image src="{{avatarUrl}}"></image></block></view><view class="login-tel"><image src="../../static/login-tel.png"></image><input placeholder-class="place-holder" type="text" value="{{account}}" maxlength="11" placeholder="请输入登录账号" data-event-opts="{{[['input',[['__set_model',['','account','$event',[]]]]]]}}" bindinput="__e"/></view><view class="login-psw"><image class="login-psw-psw" src="../../static/login-psw.png"></image><block wx:if="{{inputType==='checkbox'}}"><input placeholder-class="place-holder" value="" placeholder="请输入密码" type="checkbox" data-event-opts="{{[['change',[['e0',['$event']]]]]}}" checked="{{$root.g0?_i(password,'')>-1:password}}" bindchange="__e"/></block><block wx:else><block wx:if="{{inputType==='radio'}}"><input placeholder-class="place-holder" value="" placeholder="请输入密码" type="radio" data-event-opts="{{[['change',[['e1',['$event']]]]]}}" checked="{{_q(password,'')}}" bindchange="__e"/></block><block wx:else><input placeholder-class="place-holder" value="{{password}}" placeholder="请输入密码" type="{{inputType}}" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" bindinput="__e"/></block></block><block wx:if="{{!showPassword}}"><image class="login-psw-eye" src="../../static/login-hideeye.png" data-event-opts="{{[['tap',[['showpsw']]]]}}" bindtap="__e"></image></block><block wx:if="{{showPassword}}"><image class="login-psw-eye" src="../../static/login-eye.png" data-event-opts="{{[['tap',[['hidepsw']]]]}}" bindtap="__e"></image></block></view><view class="login-submit"><button open-type="getUserInfo" data-event-opts="{{[['tap',[['login']]]]}}" bindtap="__e">登录</button></view></view></view>